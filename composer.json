{
    "name": "rockbuzz/lara-comments",
    "description": "Comments is a package for managing comments on features like Posts",
    "keywords": [
        "laravel",
        "comments",
        "like"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "TiagoDevWeb",
            "email": "tiago@rockbuzz.com.br"
        },
        {
            "name": "Paulo Henrique P. Scheuermann",
            "email": "paulo@rockbuzz.com.br"
        }
    ],
    "autoload": {
        "psr-4": {
            "Rockbuzz\\LaraComments\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=7.2",
        "bensampo/laravel-enum": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "orchestra/testbench": "^4.0",
        "squizlabs/php_codesniffer": "^3.2"
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "cs": "vendor/bin/phpcs"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Rockbuzz\\LaraComments\\ServiceProvider"
            ]
        }
    }
}